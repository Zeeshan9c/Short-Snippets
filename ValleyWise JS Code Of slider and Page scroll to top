$(window).scroll(function() { 
    if ($('.top-tabs-sections .elementor-icon-list-item.active').length > 0) {
        var scrolledLiText = $('.top-tabs-sections .elementor-icon-list-item.active a .elementor-icon-list-text').text();
        // Update the text of the span
        $('.top-tabs-sections .elementor-widget-icon-list .list_menu_bar').text(scrolledLiText);
    } else {
        var firstLiText = $('.top-tabs-sections .elementor-icon-list-items .elementor-icon-list-item:first-child').text();
        $('.top-tabs-sections .elementor-widget-icon-list .list_menu_bar').text(firstLiText);
    }
});

	
	
	








