					$(document).ready(function () {
    $(document).on('click', '.offers-container-wrapper .elementor-widget-call-to-action:not(.contact_info) .elementor-cta__title', function (e) {
			e.stopPropagation();
  if (window.matchMedia("(max-width: 767px)").matches == true) {
		
 $(this).next().slideToggle();
			
 $(this).toggleClass('active');
 $(this).closest('.elementor-widget-call-to-action').siblings().find('.elementor-cta__title').removeClass('active').next().slideUp();
      var closestParent = $(this).closest('.elementor-widget-call-to-action');
      setTimeout(function () {
        $('html, body').animate({
          scrollTop: closestParent.offset().top - 180
        }, 500);
      }, 500); // Add the setTimeout with a delay of 500 milliseconds
		
  }
    });					 



						 
						 
						 
						 
						 
$(".wpb-accordions .e-n-accordion-item-title").on('click',function() {
     let closestParent = $(this).closest('.e-n-accordion-item ');
   setTimeout(function () {
  $('html, body').animate({
    scrollTop: $(closestParent).offset().top - 180
  }, 500);
   }, 500); // Add the setTimeout with a delay of 500 milliseconds
});

			

});
	

	
	
	
// Get the text of the first li element and set it to the span initially
var firstLiText = $('.top-tabs-sections .elementor-icon-list-items .elementor-icon-list-item:first-child').text();
$('.top-tabs-sections .elementor-widget-icon-list .elementor-widget-container').append('<div class="wrapper_menu_bar"><span class="list_menu_bar">' + firstLiText + '</span></div>');

$('.top-tabs-sections .elementor-widget-icon-list .wrapper_menu_bar').on('click', function() {
  $('.top-tabs-sections .elementor-icon-list-items').slideToggle();
});
	

	


 $(document).ready(function () {
            
	 
		$(".top-tabs-sections .elementor-icon-list-item a").on("click", function(e) {
				e.preventDefault(); // Prevent the default behavior of jumping to the anchor
				e.stopPropagation();

  var clickedLiText = $(this).text();
  
  // Update the text of the span
  $('.top-tabs-sections .elementor-widget-icon-list .list_menu_bar').text(clickedLiText);
	let target = $(this).attr("href");
	// Slide up the toggled elements
	
	if(window.matchMedia('(max-width: 767px)').matches == true) {
		$('.top-tabs-sections .elementor-icon-list-items').slideUp(400,function(){
			
		let headerHeight = $('.mobile-fixed').height();
			if($(target).length > 0){
		$("html, body").animate({
						scrollTop: $(target).offset().top - headerHeight - 20 // Adjust the offset as needed
				}, 500); // Adjust the duration as needed
		}
		});
	} else {
		let headerHeight = 80;
		if($(target).length > 0){
		$("html, body").animate({
						scrollTop: $(target).offset().top - headerHeight - $(".top-tabs-sections").height() // Adjust the offset as needed
				}, 500); // Adjust the duration as needed
		}
	}
		});
	 
});




	
	

	
	$(window).on("load resize", function () {    
        $(window).scroll(function() {
            var scrollPosition = $(window).scrollTop();
            var screenWidth = $(window).width();
            if (screenWidth <= 991) {
                if (scrollPosition > 300) {
                    $(".top-tabs-sections").css("top", "135px");
                } else {
                    $(".top-tabs-sections").css("top", "135px"); // This line can be removed since it sets the same value as the previous line.
                }
            } else {
                if (scrollPosition > 300) {
                    $(".top-tabs-sections").css("top", "130px");
                } else {
                    $(".top-tabs-sections").css("top", "180px");
                }
            }
        });
});
	


	const $sections = $("section[id]");
	$(window).on("scroll", function() {
		
			let scrollY = $(window).scrollTop();

			$sections.each(function() {
				const $current = $(this);
				const sectionHeight = $current.outerHeight();
				const sectionTop = $current.offset().top - $('.desktop-nav-wrapper').height() - 45;
				const sectionId = $current.attr("id");

				if (scrollY > sectionTop && scrollY <= sectionTop + sectionHeight) {
					// Add the "active" class to the parent <li> element 
					$(".top-tabs-sections .elementor-icon-list-items li:has(a[href*=" + sectionId + "])").addClass("active");
				} else {
					// Remove the "active" class from the parent <li> element
					$(".top-tabs-sections .elementor-icon-list-items li:has(a[href*=" + sectionId + "])").removeClass("active");
						
				}
			});

	});
	
	
