$(window).on("load resize", function () {
    if ($(".services-slider-wrapper").length) {
        if (window.matchMedia("(max-width: 767px)").matches) {
            $(".services-slider-wrapper").addClass("swiper");
            $(".services-slider-wrapper > .e-con-inner").addClass("swiper-wrapper");
            $(".services-slider-wrapper > .e-con-inner > .slider-cta-wrapper").addClass("swiper-slide");

            if (cs_swiper?.destroyed === true || !cs_swiper) {
                cs_swiper = new Swiper(".services-slider-wrapper", {
                    slidesPerView: 1.75,
                    loop: false,
                    spaceBetween: 10,
                    grabCursor: true,
                    breakpoints: { 
                        499: {
                            slidesPerView: 2.2,
                            spaceBetween: 15,
                        },
                    },
                },
              );
            }
        } else {
            if (cs_swiper !== undefined) cs_swiper.destroy(true, true);
            $(".services-slider-wrapper").removeClass(
                "swiper  swiper-container-initialized "
            );
            $(
                ".services-slider-wrapper > .e-con-inner"
            ).removeClass("swiper-wrapper");
            $(
                ".services-slider-wrapper > .e-con-inner > .slider-cta-wrapper"
            ).removeClass("swiper-slide");
        }
    }
						 
						 
